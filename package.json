{
  "name": "vite-starter-template",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:shnparker/vite-starter-template.git"
  },
  "author": {
    "name": "Shane Parker",
    "email": "hey@shnparker.com",
    "url": "https://shnparker.com",
    "github": "shnparker"
  },
  "description": "A template allowing for rapid development of React SPA projects based on Vite.",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write . --loglevel warn",
    "format:check": "prettier --check . --loglevel warn",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepare": "if [[ ! $NODE_ENV == \\\"production\\\" ]];then husky install; fi"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.27.0"
  },
  "devDependencies": {
    "@headlessui/react": "1.7.19",
    "@heroicons/react": "2.1.5",
    "@tailwindcss/forms": "0.5.9",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.16.13",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.3.3",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-react": "7.37.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "13.3.0",
    "postcss": "8.4.47",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "vite": "4.5.5",
    "vitest": "0.34.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,yml,md,mdx,html}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
