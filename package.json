{
  "name": "vite-starter-template",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:shnparker/vite-starter-template.git"
  },
  "author": {
    "name": "Shane Parker",
    "email": "hey@shnparker.com",
    "url": "https://shnparker.com",
    "github": "shnparker"
  },
  "description": "A template allowing for rapid development of React SPA projects based on Vite.",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write . --loglevel warn",
    "format:check": "prettier --check . --loglevel warn",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepare": "if [[ ! $NODE_ENV == \\\"production\\\" ]];then husky install; fi"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.11.1"
  },
  "devDependencies": {
    "@headlessui/react": "1.7.14",
    "@heroicons/react": "2.0.17",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.1.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitejs/plugin-react": "4.0.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "jsdom": "22.0.0",
    "lint-staged": "13.2.2",
    "postcss": "8.4.23",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.2.8",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vitest": "0.31.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,yml,md,mdx,html}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
